<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>User_CRUD</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.zhoooxy.crud.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>GB18030</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>UserAddServlet</servlet-name>
    <servlet-class>com.zhoooxy.crud.servlet.UserAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserAddServlet</servlet-name>
    <url-pattern>/user_add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserLogServlet</servlet-name>
    <servlet-class>com.zhoooxy.crud.servlet.UserLogServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLogServlet</servlet-name>
    <url-pattern>/user_log</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserModServlet</servlet-name>
    <servlet-class>com.zhoooxy.crud.servlet.UserModServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserModServlet</servlet-name>
    <url-pattern>/user_mod</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserpwdModServlet</servlet-name>
    <servlet-class>com.zhoooxy.crud.servlet.UserPwdModServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserpwdModServlet</servlet-name>
    <url-pattern>/user_pwdmod</url-pattern>
  </servlet-mapping>
</web-app>